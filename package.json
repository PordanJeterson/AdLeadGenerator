{
  "name": "receiptreader",
  "version": "1.0.0",
  "description": "Ingests receipts, itemizes them, and generates a weekly expense report",
  "scripts": {
    "start": "node dist/server.js",
    "build": "npm run build-ts-server && npm run tslint && npm run webpack-prod",
    "build-ts-server": "tsc",
    "debug": "npm run build && npm run watch-debug",
    "dev": "npm run watch",
    "nodemon": "nodemon ./dist/server.js",
    "serve-debug": "nodemon --inspect dist/server/server.js",
    "tslint": "tslint --fix -c tslint.json -p tsconfig.json",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Webpack,Typescript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-webpack\" \"npm run watch-ts\" \"npm run nodemon\"",
    "watch-ts": "tsc -w",
    "watch-webpack": "webpack --watch",
    "webpack-prod": "webpack --env.NODE_ENV=production --config webpack.config.babel.js",
    "webpack": "webpack --env.NODE_ENV=development --config webpack.config.babel.js"
  },
  "dependencies": {
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "errorhandler": "^1.5.0",
    "@material-ui/core": "^1.5.0",
    "@material-ui/icons": "^2.0.2",
    "node-fetch": "^2.2.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-hot-loader": "^4.3.4",
    "react-router": "^4.3.1 ",
    "react-router-dom": "^4.3.1",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "@types/dotenv": "^4.0.3",
    "@types/errorhandler": "^0.0.32",
    "@types/express": "^4.16.0",
    "@types/material-ui": "^0.21.5",
    "@types/node": "^10.7.0",
    "@types/react": "^16.4.11",
    "@types/react-dom": "^16.0.7",
    "@types/react-router-dom": "^4.3.0",
    "@types/webpack-env": "^1.13.6",
    "awesome-typescript-loader": "^5.2.0",
    "babel-loader": "^7.1.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "browser-sync": "^2.24.6",
    "browser-sync-webpack-plugin": "^2.2.2",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^3.6.1",
    "copy-webpack-plugin": "^4.5.2",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.9.3",
    "nodemon": "^1.18.3",
    "source-map-loader": "^0.2.4",
    "tslint": "^5.11.0",
    "tslint-loader": "^3.6.0",
    "typescript": "^3.0.1",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0"
  },
  "author": "Alec Hansen",
  "license": "UNLICENSED"
}
